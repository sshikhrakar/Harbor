{
  "name": "TestHarbor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "precommit": "npm run lint; npm test",
    "test": "jest -u",
    "test:watch": "jest -u --watch",
    "test:watch:cov": "jest -u --watch --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.1",
    "react-native-firebase": "^2.1.4",
    "react-native-fs": "^2.5.1",
    "react-native-navigation": "1.1.179",
    "react-redux": "5.0.5",
    "recompose": "0.24.0",
    "redux": "3.7.2",
    "redux-observable": "0.14.1",
    "redux-persist": "4.8.3",
    "rxjs": "5.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "2.1.0",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.54.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "prop-types": "^15.5.10",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "./jest-config.js"
    ]
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
